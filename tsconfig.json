{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "rootDirs": [
      "src/"
    ],
    "baseUrl": "."
  },
  "include": [
    "src/"
  ],
  "exclude": [
    "node_modules"
  ]
}
